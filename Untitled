          '<div><span class="seconds">%S</span><br/><span class="secondsSubtitle">Seconds</span></div>')).on('finish.countdown', function() {
                $('#day1Wrapper').hide();
          $('#day1image').hide();
          $('#unlock1').css('display', 'block');
        });
      });
    }

    fPop();

