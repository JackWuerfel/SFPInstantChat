  <script>
    function ePop() {
      var eventPop = moment.tz("2018-11-06 19:50", "America/New_York");

      $('#clock').countdown(eventPop.toDate(), function(event) {
        var t = $(this).html(event.strftime('' +
          '<div><span class="days">%D</span><br/><span class="daySubtitle">Days</span></div>' +
          '<div><span class="hours">%H</span><br/><span class="hoursSubtitle">Hours</span></div>' +
          '<div><span class="hours">%M</span><br/><span class="minutesSubtitle">Minutes</span></div>' +
          '<div><span class="seconds">%S</span><br/><span class="secondsSubtitle">Seconds</span></div>')).on('finish.countdown', function() {
          $('.clockwrapper').hide();
          $('.liveEvent').css('display', 'block');
        });
      });
    }

    ePop();

    function fPop() {
      var eventPop = moment.tz("2018-10-30 10:00", "America/New_York");

      $('#clock1').countdown(eventPop.toDate(), function(event) {
